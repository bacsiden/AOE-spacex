@{
    var data = AOE.TruongAn.Web.Controllers.HomeController.GetData(ViewContext.HttpContext.Request, "content");
    var aboutUs = AOE.TruongAn.Web.Controllers.HomeController.GetData(ViewContext.HttpContext.Request, "about-us");
    var service = AOE.TruongAn.Web.Controllers.HomeController.GetData(ViewContext.HttpContext.Request, "services");
}
<!--==========================
          Featured Services Section
        ============================-->
<section id="featured-services">
    <div class="container">
        <div class="row">
            @foreach (dynamic item in data.chooseUs.items)
            {
                <div class="col-lg-4 box">
                    <i class="@item.icon"></i>
                    <h4 class="title"><a href="">@item.title</a></h4>
                    <p class="description">@item.title1</p>
                </div>
            }
        </div>
    </div>
</section><!-- #featured-services -->
<!--==========================
  About Us Section
============================-->
<section id="about">
    <div class="container">

        <header class="section-header">
            <h3>@aboutUs.title</h3>
            <p>@aboutUs.title1</p>
        </header>

        <div class="row about-cols">
            @foreach (dynamic item in aboutUs.items)
            {
                <div class="col-md-4 wow fadeInUp">
                    <div class="about-col">
                        <div class="img">
                            <img src="@item.img" alt="" class="img-fluid">
                            <div class="icon"><i class="@item.icon"></i></div>
                        </div>
                        <h2 class="title"><a href="#">@item.title</a></h2>
                        <p>
                            @item.text
                        </p>
                    </div>
                </div>
            }
        </div>

    </div>
</section><!-- #about -->
<!--==========================
  Services Section
============================-->
<section id="services">
    <div class="container">

        <header class="section-header wow fadeInUp">
            <h3>@service.title</h3>
            <p>@service.title1</p>
        </header>

        <div class="row">
            @foreach (dynamic item in service.items)
            {
                <div class="col-lg-4 col-md-6 box wow bounceInUp" data-wow-duration="1.4s">
                    <div class="icon"><i class="@item.icon"></i></div>
                    <h4 class="title"><a href="">@item.title</a></h4>
                    <p class="description">@item.title1</p>
                </div>
            }
        </div>

    </div>
</section><!-- #services -->
