<section id="image-block" class="image-block no-padding">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-6 ts-padding" style="height:650px;background:url(/images/image-block-bg.jpg) 50% 50% / cover no-repeat;">
            </div>
            <div class="col-md-6 ts-padding img-block-right">
                <div class="img-block-head text-center">
                    <h2>@data.chooseUs.title0</h2>
                    <h3>@data.chooseUs.title</h3>
                    <p>
                        @data.chooseUs.title1
                    </p>
                </div>

                <div class="gap-30"></div>
                @foreach (var item in data.service.services)
                {
                    <div class="image-block-content">
                        <span class="feature-icon pull-left"><i class="@item.icon"></i></span>
                        <div class="feature-content">
                            <h3>@item.title</h3>
                            <p>@item.title1</p>
                        </div>
                    </div>
                }
            </div>
        </div>
    </div>
</section><!--/ Image block end -->
