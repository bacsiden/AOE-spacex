<section id="service" class="service angle">
    <div class="container">
        <div class="row">
            <div class="col-md-12 heading">
                <span class="title-icon pull-left"><i class="fa fa-cogs"></i></span>
                <h2 class="title">@data.service.title <span class="title-desc">@data.service.title1</span></h2>
            </div>
        </div><!-- Title row end -->

        <div class="row">
            <div class="col-md-12">
                @foreach (var item in data.service.services)
                {
                    <div class="col-md-3 col-sm-3 wow fadeInDown" data-wow-delay=".5s">
                        <div class="service-content text-center">
                            <span class="service-icon icon-pentagon"><i class="@item.icon"></i></span>
                            <h3>@item.title</h3>
                            <p>@item.title1</p>
                        </div>
                    </div>
                }
            </div>
        </div><!-- Content row end -->
    </div><!--/ Container end -->
</section><!--/ Service box end -->
