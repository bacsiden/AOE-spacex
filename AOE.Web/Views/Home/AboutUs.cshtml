@{
    var dataUs = AOE.Web.Controllers.HomeController.GetData(ViewContext.HttpContext.Request, "about-us");
    var first = true;
    var data = AOE.Web.Controllers.HomeController.GetData(ViewContext.HttpContext.Request, "content");
}
<div id="banner-area">
    <img src="/images/banner/banner1.jpg" alt="" />
    <div class="parallax-overlay"></div>
    <!-- Subpage title start -->
    <div class="banner-title-content">
        <div class="text-center">
            <h2>About Us</h2>
            <ul class="breadcrumb">
                <li>Home</li>
                <li><a href="#"> About Us</a></li>
            </ul>
        </div>
    </div><!-- Subpage title end -->
</div><!-- Banner area end -->
<!-- Main container start -->

<section id="main-container">
    <div class="container">

        <!-- Company Profile -->
        <div class="row">
            <div class="col-md-12 heading">
                <span class="title-icon classic pull-left"><i class="fa fa-suitcase"></i></span>
                <h2 class="title classic">@dataUs.title</h2>
            </div>
        </div><!-- Title row end -->

        <div class="row">
            <div class="landing-tab clearfix">
                <ul class="nav nav-tabs nav-stacked col-md-3 col-sm-5">
                    @foreach (var item in dataUs.items)
                    {
                        <li class="@(first?"active": null)">
                            <a class="animated fadeIn" href="#@item.id" data-toggle="tab">
                                <span class="tab-icon"><i class="@item.icon"></i></span>
                                <div class="tab-info">
                                    <h3>@item.titleLeft</h3>
                                </div>
                            </a>
                        </li>
                        first = false;
                    }
                </ul>

                <div class="tab-content col-md-9 col-sm-7">
                @{ 
                    first = true;
                }

                    @foreach (var item in dataUs.items)
                    {
                        <div class="tab-pane @(first?"active": null) animated fadeInRight" id="@item.id">
                            <i class="fa fa-trophy big"></i>
                            <h3>@item.title</h3>
                            <p>@item.text</p>
                        </div>
                        first = false;
                    }
                </div><!-- tab content -->
            </div><!-- Overview tab end -->
        </div><!--/ Content row end -->
        <!-- Company Profile -->

    </div><!--/ 1st container end -->

    <div class="gap-60"></div>

    <div class="container">
        <div class="row">
            <div class="col-md-12 heading">
                <span class="title-icon pull-left"><i class="fa fa-weixin"></i></span>
                <h2 class="title">@data.meetTeam.title <span class="title-desc">@data.meetTeam.title1</span></h2>
            </div>
        </div><!-- Title row end -->

        <div class="row text-center">
            @foreach (var item in data.meetTeam.persons)
            {
                <div class="col-md-3 col-sm-6">
                    <div class="team wow slideInLeft">
                        <div class="img-hexagon">
                            <img src="@item.img" alt="">
                            <span class="img-top"></span>
                            <span class="img-bottom"></span>
                        </div>
                        <div class="team-content">
                            <h3>@item.name</h3>
                            <p>@item.title</p>
                            @*<div class="team-social">
                                <a class="fb" href="#"><i class="fa fa-facebook"></i></a>
                                <a class="twt" href="#"><i class="fa fa-twitter"></i></a>
                                <a class="gplus" href="#"><i class="fa fa-google-plus"></i></a>
                                <a class="linkdin" href="#"><i class="fa fa-linkedin"></i></a>
                                <a class="dribble" href="#"><i class="fa fa-dribbble"></i></a>
                            </div>*@
                        </div>
                    </div>
                </div>
            }
        </div><!--/ Content row end -->
    </div>
</section><!--/ Main container end -->